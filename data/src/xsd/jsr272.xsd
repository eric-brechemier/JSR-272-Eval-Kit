        <xs:appinfo source="eric.brechemier.name/2008-01-21/jsr272/CommonMetadataSet">SERVICE_IS_PROTECTED</xs:appinfo>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  
