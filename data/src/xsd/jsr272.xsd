        <xs:annotation>
          <xs:appinfo source="eric.brechemier.name/2008-01-21/jsr272/CommonMetadataSet">PURCHASE_CHANNEL_NAME</xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="description" type="xs:string" min0ccurs="0" max0ccurs="unbounded">
