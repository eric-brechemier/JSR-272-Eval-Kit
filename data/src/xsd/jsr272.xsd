      <xs:element name="auxiliaryClip" type="jsr272:URI" min0ccurs="0" max0ccurs="unbounded">
        <xs:annotation>
          <xs:appinfo source="eric.brechemier.name/2008-01-21/jsr272/CommonMetadataSet">SERVICE_AUX_CLIP</xs:appinfo>
