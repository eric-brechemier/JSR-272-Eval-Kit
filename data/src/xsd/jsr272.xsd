        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="id" type="jsr272:URI" use="optional">
      <xs:annotation>
        <xs:appinfo source="eric.brechemier.name/2008-01-21/jsr272/CommonMetadataSet">PURCHASE_ID</xs:appinfo>
